METHOD 1:

    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int maxi=0;
        int mini=INT_MAX;
        int profit=0;
        for(int i=0;i<n;i++){
            mini=min(mini, prices[i]);
            profit=prices[i]-mini;
            maxi=max(maxi, profit);
        }
        return maxi;
    }

T.C. = O(n)
S.C. = O(1)
