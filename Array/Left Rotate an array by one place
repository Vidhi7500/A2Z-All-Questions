METHOD 1:

#include<bits/stdc++.h>
using namespace std;

void reverse(vector<int>& v, int start, int end){
    while(start<end){
        swap(v[start], v[end]);
        ++start;
        --end;
    }
}

void left(vector<int>& v, int k){
    int n=v.size();
    reverse(v, 0, n-1);
    reverse(v, 0, k-1);
    reverse(v, k, n-1);
}

int main()
{
    int n;
    cin>>n;
    vector<int> v;
    for(int i=0;i<n;i++){
        int x;
        cin>>x;
        v.push_back(x);
    }
    left(v, 1);
    for(int i=0;i<n;i++){
        cout<<v[i]<<" ";
    }
    return 0;
}

T.C. = O(n)
S.C. = O(1)
